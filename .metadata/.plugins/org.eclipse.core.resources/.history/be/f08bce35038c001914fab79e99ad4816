package com.bridgelabz.redis.repository;

import java.util.List;
import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.bridgelabz.redis.model.Note;

@Repository
public interface NoteRadisRepository extends JpaRepository<Note, Long>{
	
	Optional<Note> findByNoteIdAndUserId(long noteId,long userId);
	List<Note> findNoteByUserId(long userId);
	
}
